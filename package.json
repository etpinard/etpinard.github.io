{
  "name": "etpinard.github.io",
  "version": "1.0.0",
  "description": "Personal web page / page web personnelle",
  "scripts": {
    "lint": "standard --fix",
    "test": "standard | snazzy",
    "start": "budo src/index.js --serve bundle.js --open --live",
    "disc": "browserify src/index.js --full-paths | discify --open",
    "build:css": "cp -f node_modules/octicons/build/octicons.min.css .",
    "build:js": "browserify src/index.js | uglifyjs -m -c > bundle.js",
    "build": "npm run build:css && npm run build:js"
  },
  "browserify": {
    "transform": [
      "yamlify",
      "es2020",
      "yo-yoify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etpinard/etpinard.github.io.git"
  },
  "author": "Étienne Tétreault-Pinard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/etpinard/etpinard.github.io/issues"
  },
  "homepage": "https://github.com/etpinard/etpinard.github.io",
  "dependencies": {
    "es2020": "^1.1.9",
    "octicons": "^5.0.1",
    "query-string": "^4.3.2",
    "yamlify": "^0.1.3",
    "yo-yo": "^1.4.0",
    "yo-yoify": "^3.5.0"
  },
  "devDependencies": {
    "browserify": "^14.1.0",
    "budo": "^9.4.7",
    "disc": "^1.3.2",
    "snazzy": "^6.0.0",
    "standard": "^9.0.0",
    "uglify-js": "^3.0.15"
  }
}
